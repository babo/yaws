
# conf for yaws


# first we have a set of globals

logdir = .
ebin_dir = %yawsdir%/examples/ebin
include_dir = %yawsdir%/examples/include
keepalive_timeout = 15000



#end then a set of servers



# enter  hostname and IP here
<server localhost>
	port = 8000
	listen = 127.0.0.1
	docroot = %yawsdir%/www
</server>




